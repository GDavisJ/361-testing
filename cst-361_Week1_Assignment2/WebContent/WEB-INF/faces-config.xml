<?xml version="1.0" encoding="UTF-8"?>
<faces-config
    xmlns="http://xmlns.jcp.org/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_3.xsd"
    version="2.3">


	<navigation-rule>
        <from-view-id>/TestForm.xhtml</from-view-id>
        <navigation-case>
            <from-action>#{testingMain.printSingleton}</from-action>
            <from-outcome>success</from-outcome>
            <to-view-id>/TestForm.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-action>#{testingMain.getUsers}</from-action>
            <from-outcome>success</from-outcome>
            <to-view-id>/TestForm.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-action>#{testingMain.printNames}</from-action>
            <from-outcome>success</from-outcome>
            <to-view-id>/TestForm.xhtml</to-view-id>
        </navigation-case>
 
    </navigation-rule>
</faces-config>
